{
	"name": "RevenueDataSink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat('RevenueData_Starting_',adddays(startOfMonth(utcNow()),-1,'ddMMyyyy'),'_Ending_',utcNow(),'.csv')",
					"type": "Expression"
				},
				"fileSystem": "sinksecond"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}